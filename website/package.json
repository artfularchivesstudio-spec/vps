    "mcp:google-sheets": "node scripts/google-sheets-mcp-server.js",
    "test:google-sheets": "node scripts/test-google-sheets-mcp.js",
    "types:generate": "node scripts/generate-openapi-types.js",
    "types:watch": "nodemon --watch config/openapi --ext yaml,yml,json --exec 'npm run types:generate'",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "dev:full": "npm run types:generate && npm run prisma:generate && npm run dev",
    "test:e2e": "playwright test",
    "test:e2e:admin": "playwright test --project=admin-flow-e2e",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:screenshots": "playwright test --project=admin-flow-e2e --project=desktop-screenshots",
    "playwright:install": "playwright install"
