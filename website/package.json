    "deploy:dry-run": "./scripts/post-merge-deploy.sh --dry-run",
    "deploy:help": "./scripts/post-merge-deploy.sh --help",
    "pre-deploy": "npm run lint && npm test && npm run build",
    "supabase:deploy": "supabase functions deploy --no-verify-jwt && supabase db push",
